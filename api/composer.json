{
  "name": "phpgrc/api",
  "description": "phpGRC Laravel API skeleton",
  "type": "project",
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0",
    "laravel/sanctum": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
